@model IEnumerable<DonationPage.Models.DonationEntry>

@{
    ViewBag.Title = "Approved stories";
    Layout = null;
}

<h2>Approved stories</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Honoree)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comments)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Honoree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comments)
            </td>
        </tr>
    }

</table>


